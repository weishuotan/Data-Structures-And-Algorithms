class Solution {
    public ListNode removeNthFromEnd(ListNode head, int n) {
        if(head==null){
            return null;
        }
        ListNode dummy=new ListNode(0,head);
        ListNode slow = dummy;
        ListNode fast = head;

        while(n>0){
            fast=fast.next;
            n--;
        }

        while(fast!=null){
            fast=fast.next;
            slow=slow.next;
        }

        slow.next=slow.next.next;
        return dummy.next;

    }
}

        return ans;

    }
}
